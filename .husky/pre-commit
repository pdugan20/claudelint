# Check for emojis (strict - only in headings)
npm run check:emojis

# Verify rule implementations
npm run verify:rule-implementations

# Run validator tests (without coverage - that's for CI)
npx jest tests/validators --passWithNoTests

# Check rule coverage
npm run check:rule-coverage

# Lint validator files
npx eslint src/validators/**/*.ts src/schemas/**/*.ts --fix

# Check for duplicate logic
npm run check:duplicates
